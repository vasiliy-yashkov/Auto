  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    p.provider_name,
    p.provider_info,
    a.auto_full,
    DELIVERY_DATE,
    DELIVERY_PRICE
FROM
DELIVERY d
join "AUTO" a on a.auto_id = d.auto_id
join PROVIDER p on p.provider_id = d.provider_id</CommandText>
      </Query>
      <Fields>
